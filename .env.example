# Notion API Credentials
# Get this from https://www.notion.so/my-integrations
NOTION_TOKEN=your_notion_integration_token

# Notion Database ID 
# The ID from your Notion database URL: https://www.notion.so/[workspace]/[database_id]?v=...
NOTION_DATABASE_ID=your_database_id

# Optional settings
# Set to DEBUG to get more verbose logging
LOG_LEVEL=INFO 